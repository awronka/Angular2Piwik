  "scripts": {
    "prepublish" : "tsc",
    "rimraf": "rimraf",
    "lint": "tslint ./*.ts ./src/{,*/}*.ts",
    "test": "tsc && karma start",
    "test-watch": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
